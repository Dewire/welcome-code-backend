{
  "name": "welcome-backend",
  "version": "1.0.0",
  "description": "Welcome AWS serverless",
  "scripts": {
    "start:local": "env $(cat .env.local | grep -v ^# | xargs) sls offline --stage cont --encPass foo --inMemory",
    "build": "yarn build:base && yarn build:dbApi && yarn build:frontendApi && yarn build:previewApi && yarn build:imageStoreApi && yarn build:onboardingApi",
    "build:base": "sls package",
    "build:dbApi": "cd dbApi && sls package && cd ..",
    "build:frontendApi": "cd frontendApi && sls package && cd ..",
    "build:previewApi": "cd previewApi && sls package && cd ..",
    "build:imageStoreApi": "cd imageStoreApi && sls package && cd ..",
    "build:onboardingApi": "cd onboardingApi && sls package && cd ..",
    "deploy": "yarn deploy:base && yarn deploy:dbApi && yarn deploy:frontendApi && yarn deploy:previewApi && yarn deploy:imageStoreApi && yarn deploy:onboardingApi",
    "deploy:base": "sls deploy --package .serverless",
    "deploy:dbApi": "cd dbApi && sls deploy --package .serverless && cd ..",
    "deploy:frontendApi": "cd frontendApi && sls deploy --package .serverless && cd ..",
    "deploy:previewApi": "cd previewApi && sls deploy --package .serverless && cd ..",
    "deploy:imageStoreApi": "cd imageStoreApi && sls deploy --package .serverless && cd ..",
    "deploy:onboardingApi": "cd onboardingApi && sls deploy --package .serverless && cd ..",
    "build:cont": "env $(cat .env.cont | grep -v ^# | xargs) yarn build",
    "build:dev": "env $(cat .env.dev | grep -v ^# | xargs) yarn build",
    "build:prod": "env $(cat .env.prod | grep -v ^# | xargs) yarn build",
    "deploy:cont": "env $(cat .env.cont | grep -v ^# | xargs) yarn deploy",
    "deploy:dev": "env $(cat .env.dev | grep -v ^# | xargs) yarn deploy",
    "deploy:prod": "env $(cat .env.prod | grep -v ^# | xargs) yarn deploy",
    "test": "npm run test:ut",
    "test:ut": "npm run lint && jest --env node --coverage",
    "lint": "eslint api libs dbApi frontendApi imageStoreApi onboardingApi previewApi",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-jest": "^21.2.0",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-import-resolver-babel-module-alias": "^1.5.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.4.1",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "jest": "^21.2.1",
    "jwt-decode": "^2.2.0",
    "patch-package": "^5.1.1",
    "postinstall-postinstall": "^1.0.0",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-apigwy-binary": "^0.1.0",
    "serverless-webpack": "^2.2.0",
    "webpack": "^3.3.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/.serverless/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "author": "Dewire",
  "license": "GPL",
  "private": true,
  "dependencies": {
    "amazon-cognito-identity-js": "^1.31.0",
    "aws-sdk": "^2.205.0",
    "axios": "^0.17.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-source-map-support": "^2.0.1",
    "babel-runtime": "^6.26.0",
    "debug": "^3.1.0",
    "deep-map": "^1.5.0",
    "dynamoose": "0.8.7",
    "eslint-plugin-babel": "^4.1.2",
    "serverless-api-stage": "^1.3.0",
    "serverless-dynamodb-local": "^0.2.26",
    "serverless-offline": "^3.16.0",
    "serverless-secrets-plugin": "^0.1.0",
    "sha1": "^1.1.1",
    "shortid": "^2.2.8",
    "sjcl": "^1.0.7",
    "uuid": "^3.2.1",
    "whatwg-fetch": "^2.0.3"
  }
}
